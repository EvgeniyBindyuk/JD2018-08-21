<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0"
         metadata-complete="true">
    <servlet>
        <servlet-name>servletFrontController</servlet-name>
        <servlet-class>by.it.artemliashkov.project.java.FrontController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>servletFrontController</servlet-name>
        <url-pattern>/do</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>servletCacheControl</filter-name>
        <filter-class>by.it.artemliashkov.project.java.CacheControl</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>servletCacheControl</filter-name>
        <url-pattern>/do</url-pattern>
    </filter-mapping>

</web-app>